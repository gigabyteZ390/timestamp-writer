{
  "manifest_version": 3,
  "name": "timestamp-writer",
  "version": "1.0",
  "description": "Write the timestamp in a customized way",
  "icons": {
    "16": "icons/extension_icon@16px.png",
    "32": "icons/extension_icon@32px.png",
    "48": "icons/extension_icon@48px.png",
    "128": "icons/extension_icon@128px.png",
    "256": "icons/extension_icon@128px.png"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "icons/extension_icon@48px.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": []
}
